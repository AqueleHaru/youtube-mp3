{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "youtube-mp3",
  "title": "YouTube MP3",
  "description": "Download YouTube videos as MP3.",
  "icon": "youtube.png",
  "license": "MIT",
  "preferences": [
    {
      "name": "ytdlPath",
      "title": "YT-DLP Path",
      "description": "Path to the yt-dlp executable. You can enter `which yt-dlp` to terminal to find its path.",
      "type": "textfield",
      "required": false
    }
  ],
  "commands": [
    {
      "name": "download-video-as-mp3",
      "title": "Download Video as MP3",
      "description": "Download Video as MP3",
      "mode": "view",
      "subtitle": "YouTube MP3 Downloader",
      "icon": "youtube.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.0",
    "@raycast/utils": "^2.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "build": "ray build",
    "publish": "npx @raycast/api@latest publish",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1"
  },
  "author": "AqueleHaru",
  "platforms": [
    "Windows",
    "macOS"
  ],
  "categories": [
    "Media"
  ]
}
